<header>
  <h1>{{method}} Request Received</h1>
</header>
<div class="query-info">
  <h2>URL Query: Parameter Names and Values</h2>
  <ul>
    {{#each queryList}}
      <li>{{this.name}}:  {{this.value}}</li>
    {{/each}}
  </ul>

</div>
<div class="body-info">
  <h2>Request Body: Property Names and Values</h2>
  <ul>
    {{#each bodyList}}
      <li>{{this.name}}:  {{this.value}}</li>
    {{/each}}
  </ul>

</div>
